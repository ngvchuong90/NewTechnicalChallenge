{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "node",
      "lib": ["ESNext", "DOM"],
      "strict": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "types": ["node", "playwright"]
    },
    "include": ["**/*.ts"],
    "exclude": ["node_modules"]
  }